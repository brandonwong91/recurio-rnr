{
  "buildCommand": "expo export -p web",
  "outputDirectory": "dist/client",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@5.1.8",
      "includeFiles": "dist/server/**"
    }
  },
  "rewrites": [
    {
      "source": "/auth/callback",
      "destination": "/auth/callback"
    },
    {
      "source": "/((?!auth/callback|_next/.*).*)",
      "destination": "/api/index"
    }
  ]
}
